{{ $emb := sdict }}
{{ $e := execAdmin "prefix" }}{{ $prefix := reReplace `[\.\[\]\-\?\!\\\*\{\}\(\)\|\+\$\^]` (slice $e (add 15 (len (str $.Guild.ID))) (sub (len $e) 1)) `\$0` }}
{{ block "main" (cslice .ExecData $commands.Song $prefix $emb) }}
	{{ $cmd := index . 0 }}{{ $prf := index . 2 }}{{ $emb := index . 3 }}
	{{ range (index . 1) }}
		{{ if in ((cslice (index . 0)).AppendSlice (index . 1)) $cmd }}
			{{ $emb.Set "fields" (cslice (sdict "name" "Category:" "value" "Song")) }}
			{{ $emb.Set "title" (title (index . 0)) }}
			{{ $als := "" }}
			{{ range (index . 1) }}
				{{ $als = print $als ", " . }}
			{{ else }}
				{{ $als = ", None" }}
			{{ end }}
			{{ $emb.Set "fields" ($emb.fields.Append (sdict "name" "Aliases:" "value" (slice $als 2))) }}
			{{ $emb.Set "description" (index . 2) }}
			{{ $usg := "" }}
			{{ if gt (len .) 3 }}
				{{ $emb.Set "fields" ($emb.fields.Append (sdict "name" "Usage:" "value" (print $prf $cmd " " (index . 3)))) }}
			{{ end }}
		{{ end }}
	{{ end }}
{{ end }}

